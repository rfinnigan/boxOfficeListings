<h1>All Venues</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>
  <% @venues.each do |venue| %>
    <tr>
      <td><%= venue.name %></td>
      <td><%= link_to 'Show', venue_path(venue) %></td>
      <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
      <td><%= link_to 'Delete', venue_path(venue), method: :delete, data: {confirm: 'Are you sure you want to delete this Venue?' } %> </td>
    </tr>
  <% end %>
</table>

<%= link_to 'New Venue', new_venue_path %>
