<p>
  <strong>Room Name:</strong>
  <%= @room.roomName %>
</p>


<h2>Create a New Show:</h2>
<%= form_for([@room, @room.shows.build]) do |f| %>
  <p>
    <%= f.label :artist%><br>
    <%= f.text_field :artist %>
  </p>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
